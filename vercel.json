{
  "rewrites": [
    {
      "source": "/config",
      "destination": "/PRK CONFIG.rar",
      "has": [
        {
          "type": "host",
          "value": "api.glaive.lol"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/PRK CONFIG.rar",
      "headers": [
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=\"PRK CONFIG.rar\""
        },
        {
          "key": "Content-Type",
          "value": "application/zip"
        }
      ]
    }
  ]
}
